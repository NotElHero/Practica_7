<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="Pregunta 2"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Â¿Cuales de estos lenguajes son compilados?"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/botonSiguiente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="Siguiente"
        app:layout_constraintEnd_toEndOf="@+id/tableLayout"
        app:layout_constraintStart_toStartOf="@+id/tableLayout"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <Button
        android:id="@+id/botonAnterior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:text="Anterior"
        app:layout_constraintEnd_toEndOf="@+id/botonSiguiente"
        app:layout_constraintStart_toStartOf="@+id/botonSiguiente"
        app:layout_constraintTop_toTopOf="@+id/botonSiguiente" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="201dp"
        android:layout_height="111dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <CheckBox
                android:id="@+id/checkC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="C" />

            <CheckBox
                android:id="@+id/checkPython"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Python" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <CheckBox
                android:id="@+id/checkJS"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="JavaScript" />

            <CheckBox
                android:id="@+id/checkJava"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Java" />
        </TableRow>

    </TableLayout>
</androidx.constraintlayout.widget.ConstraintLayout>